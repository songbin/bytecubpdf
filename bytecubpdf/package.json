{
  "name": "bytecub",
  "productName": "bytecub",
  "version": "0.5.2",
  "private": true,
  "main": "dist-electron/main/index.js",
  "scripts": {
    "dev": "vite",
    "electron:dev": "concurrently -k \"vite dev\" \"wait-on http-get://localhost:5173/ && cross-env ELECTRON_DISABLE_SANDBOX=1 electron . --remote-debugging-port=9222 --disk-cache-dir=./.electron-cache\"",
    "prebuild": "npm run clean",
    "build": "npm run clean && vite build && electron-builder",
    "clean": "rimraf dist release dist-electron",
    "build:renderer": "vite build",
    "build:main": "vite build --config vite.main.config.ts",
    "build:preload": "vite build --config vite.preload.config.ts",
    "preview": "vite preview",
    "electron": "wait-on tcp:5173 && electron .",
    "lint": "eslint . --ext .vue,.js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "postinstall": "cross-env ELECTRON_MIRROR=https://npmmirror.com/mirrors/electron/ electron-rebuild",
    "rebuild": "cross-env ELECTRON_MIRROR=https://npmmirror.com/mirrors/electron/ electron-rebuild -f -w sqlite3"
  },
  "dependencies": {
    "@langchain/community": "^0.3.46",
    "@langchain/core": "^0.3.43",
    "@langchain/ollama": "^0.2.0",
    "@langchain/openai": "^0.5.4",
    "@microsoft/fetch-event-source": "^2.0.1",
    "@vicons/antd": "^0.13.0",
    "@vicons/carbon": "^0.13.0",
    "@vicons/fluent": "^0.13.0",
    "@vicons/ionicons5": "^0.13.0",
    "cross-fetch": "^4.1.0",
    "dompurify": "^3.2.5",
    "electron-updater": "^6.6.2",
    "js-sha3": "^0.9.3",
    "katex": "^0.16.22",
    "langchain": "^0.3.20",
    "marked": "^15.0.11",
    "openai": "^5.7.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "vue": "^3.3.0",
    "vue-element-plus-x": "^1.2.0",
    "vue-i18n": "^9.2.2",
    "vue-pdf-embed": "^2.1.2",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@electron-toolkit/utils": "^4.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.17.28",
    "@vicons/material": "^0.13.0",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vue/devtools": "^7.7.2",
    "@vue/eslint-config-typescript": "^11.0.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^32.2.0",
    "electron-builder": "^24.0.0",
    "electron-devtools-installer": "^3.0.0",
    "electron-rebuild": "^3.2.9",
    "eslint": "^8.0.0",
    "eslint-plugin-vue": "^9.0.0",
    "naive-ui": "^2.41.0",
    "prettier": "^2.0.0",
    "typescript": "^5.0.0",
    "vite": "^4.5.11",
    "vite-plugin-electron": "^0.11.0",
    "wait-on": "^7.2.0"
  },
  "build": null
}
